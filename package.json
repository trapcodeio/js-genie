{
  "name": "js-genie",
  "version": "1.0.1",
  "description": "Well Documented  Functions we find ourselves reinventing every-time.",
  "type": "module",
  "repository": "https://github.com/trapcodeio/js-genie.git",
  "author": "trapcodeio <trapcode.io@gmail.com>",
  "license": "MIT",
  "scripts": {
    "watch": "npx tsc --watch",
    "play": "npx ts-node-dev --respawn ./tests/play.ts",
    "build": "rm -rf ./dist && npx tsc",
    "bundle": "npx tsup",
    "prepublishOnly": "npm run build && npm run bundle",
    "generate:exports": "node --loader=ts-node/esm ./scripts/generate-exports.ts"
  },
  "exports": {
    "./array/*": {
      "import": "./dist/array/*.js",
      "require": "./dist/array/*.cjs",
      "types": "./dist/array/*.d.ts"
    },
    "./crypto/*": {
      "import": "./dist/crypto/*.js",
      "require": "./dist/crypto/*.cjs",
      "types": "./dist/crypto/*.d.ts"
    },
    "./errors/*": {
      "import": "./dist/errors/*.js",
      "require": "./dist/errors/*.cjs",
      "types": "./dist/errors/*.d.ts"
    },
    "./string/*": {
      "import": "./dist/string/*.js",
      "require": "./dist/string/*.cjs",
      "types": "./dist/string/*.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs",
      "types": "./dist/utils/*.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {}
}
